{%- assign location = site.data.locations[include.location] -%}
{
    "@context": "http://schema.org",
    "@type": "Place",
    "name": "{{ location.name }}",
    "address": {
        "@type": "PostalAddress",
        "streetAddress": "{{ location.address.street }}",
        "addressLocality": "{{ location.address.city }}",
        "postalCode": "{{ location.address.postcode }}",
        "addressCountry": "UK"
    }
}
